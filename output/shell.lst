Nvoid
NShellGetString(S8 *string)
N{
N    S8 *buf = string;
N    S8 c;
N    while((c = ShellGetByte())!='\r')
N    {
N        if(c=='\b')
N        {
N            if( (int)buf < (int)string )
N            {
N                ShellPrintf("\b \b");
N                string--;
N            }
N        }
N        else 
N        {
N            *string++ = c;
N            ShellPutByte(c);
N        }
N    }
N    *string='\0';
N    ShellPutByte('\n');
N}
