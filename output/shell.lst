NS32
NShellGetIntNum(void)
N{
N    char str[30];
N    char *string = str;
N    int base     = 10;
N    int minus    = 0;
N    int result   = 0;
N    int lastIndex;    
N    int i;
N    
N    ShellGetString(string);
N    
N    if(string[0]=='-')
N    {
N        minus = 1;
N        string++;
N    }
N    
N    if(string[0]=='0' && (string[1]=='x' || string[1]=='X'))
N    {
N        base    = 16;
N        string += 2;
N    }
N    
N    lastIndex = strlen(string) - 1;
N    
N    if(lastIndex<0)
N        return -1;
N    
N    if(string[lastIndex]=='h' || string[lastIndex]=='H' )
N    {
N        base = 16;
N        string[lastIndex] = 0;
N        lastIndex--;
N    }
N
N    if(base==10)
N    {
N        result = atoi(string);
N        result = minus ? (-1*result):result;
N    }
N    else
N    {
N        for(i=0;i<=lastIndex;i++)
N        {
N            if(isalpha(string[i]))
X            if(((* __rt_ctype_table())[string[i]] & (16+8+128)))
N            {
N                if(isupper(string[i]))
X                if(((* __rt_ctype_table())[string[i]] & 16))
N                    result = (result<<4) + string[i] - 'A' + 10;
N                else
N                    result = (result<<4) + string[i] - 'a' + 10;
N            }
N            else
N                result = (result<<4) + string[i] - '0';
N        }
N        result = minus ? (-1*result):result;
N    }
N    return result;
N}
